  },
  "config": {
    "pentaho/modules": {
      "IPenFileService": {"base": null, "isAbstract": true},
      "pentaho/di/plugin/fileOpenSaveNew/services/providers/vfs.service": {"type": "IPenFileService"},
      "pentaho/di/plugin/fileOpenSaveNew/services/providers/recents.service": {"type": "IPenFileService"},
      "pentaho/di/plugin/fileOpenSaveNew/services/providers/repository.service": {"type": "IPenFileService"},
